<%- include ("./partials/header") %>
<script src="//code.jquery.com/jquery-1.11.1.min.js"></script>
<div class="container">
  <div class="row" style="display: flex; justify-content: space-between;">
    <div class="column-md-6">
      <img
        style="
          margin-top: 45px;
          height: 400px;
          width: 500px;
          box-shadow: 5px 8px 10px #cbc6cf;
        "
        ;
        src=" https://images.unsplash.com/photo-1569336415962-a4bd9f69cd83?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=600&q=60"
      />
    </div>
    <div class="col-md-6">
      <div class="form-style-8">
        <h2 style="color: white; background-color: #2c0b4a;">Salon At Home</h2>
        <form>
          <input type="text" name="field2" placeholder="Your Location" />
          <input
            type="text"
            name="field2"
            placeholder="Flat/ Building/ Street"
          />
          <input type="text" name="field3" placeholder="Enter your full name" />

          <input
            id="submitbtn"
            type="button"
            style="
              border-radius: 10px;
              margin-top: 20px;
              padding-left: 25px;
              padding-right: 25px;
              margin-left: 150px;
              color: white;
              background-color: #2c0b4a;
            "
            value="Log In"
          />
        </form>
      </div>
    </div>
  </div>
</div>

<script type="text/javascript">
  $("#submitbtn").click(function () {
    window.location.href = "/paydetails";
  });
</script>

